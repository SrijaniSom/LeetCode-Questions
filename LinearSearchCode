class Main
{
public static void main(String args[])
{
	int[] nums={12, 23, 8, 56, 19, 20};
	int target=19;
    int res=searchfunc(nums,target);
    System.out.println(res);
	
}
		public static int searchfunc(int[] arr,int target)
		{
		if(arr.length == 0)
			return -1;
			for(int i=0;i<arr.length;i++)
			{
			if(arr[i]==target)
				return i;
			}
			
			return -1;
		}
}
