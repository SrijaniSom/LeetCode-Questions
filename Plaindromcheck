class HelloWorld {
    public static void main(String[] args) {
      String s="MAADAM";
      int start=0;
      int end=s.length()-1;
      int c=0;
      while(start<=end)
      {
        if(s.charAt(start)==s.charAt(end))
          {
              start++;
              end--;
              
          }
          else{
              c=-1;
              break;
          }
      }
        if(c==0)
        System.out.println("Palindrome");
        else
        System.out.println("Not palindrome");
    }
}
